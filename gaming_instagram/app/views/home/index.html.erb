<%= render 'layouts/header' %>
<div class="feed">
  <h1>Welcome <%= @user.username %></h1>
  <a href="/<%= @user.username %>"><img class="cameraimg" src="https://lh6.ggpht.com/2jtlQAC-KQ58_tRBu0UjT6U-8ePmGls-SDi9q8MdHm8VQajlctnR5PhacFw-403j6jw=w300" /></a>
  <%= render 'stats' %>
</div>
<% @fol.each do |x, index| %>
  <div class="feedpost">
    <a href="/<%=x.user.username%>" class="feeduser"><h2><%= x.user.username %></h2></a>
    <p><em>Game: </em><%= x.game %> </p>
    <div class="likes"> <%=x.likes %> </div>
    <a class="feedpic" href="/<%=x.user.username%>/<%=x.id%>"><img src="<%=  aws_parser(x.image.url(:large)) %>" /></a>
    <p><%= x.caption %></p>
    
  </div>
<% end %>
</div>